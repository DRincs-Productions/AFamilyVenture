msgid "[_RT_] {i}I can't waste time right now, the DeCapos will be here any minute.{/i}"
msgstr "[_RT_] {i}Não posso perder tempo agora, os Decados chegam a qualquer momento."

# game/Phone_interface.rpy:256 #-#-# translate crowdin night_warning_48ae09e0:
msgid "[_RT_] {i}It's night, I should go to sleep.{/i}"
msgstr "[_RT_] {i}É noite, tenho de ir dormir."

# game/Phone_interface.rpy:261 #-#-# translate crowdin school_warning_1afbbf5b:
msgid "[_RT_] {i}Right now I'm at School, I must first go home to advance time.{/i}"
msgstr "[_RT_] {i}Agora estou na escola, tenho de ir para casa para adiantar o tempo."

# game/Phone_interface.rpy:266 #-#-# translate crowdin campaign_warning_88e99dc5:
msgid "[_RT_] {i}I can't waist time. I need to go to school and see how Lauren is doing in the student body election poll.{/i}"
msgstr "[_RT_] {i}Eu não posso perder tempo. Eu preciso ir para escola e ver como a Lauren esta se saindo na eleição do Grêmio Estudantil.{/i}"

# game/Phone_interface.rpy:271 #-#-# translate crowdin prison_warning_5ae78a21:
msgid "[_RT_] {i}I can't waist time. I need to get ready to visit Dad in prison.{/i}"
msgstr "[_RT_] {i}Eu não posso vacilar tempo. Eu preciso-me preparar para visitar o papai na prisão. {/i}"

