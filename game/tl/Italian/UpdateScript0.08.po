# game/UpdateScript0.08.rpy:4212 #-#-# translate crowdin weekend_date_lounge_deab7f06:
msgid "[_M_] You clean up pretty well yourself."
msgstr "[_M_] Ti ripulisci abbastanza bene."

# game/UpdateScript0.08.rpy:4213 #-#-# translate crowdin weekend_date_lounge_d54dc0f2:
msgid "[_M_] What a handsome young man you've become!"
msgstr "[_M_] Che bel giovane uomo che sei diventato!"

# game/UpdateScript0.08.rpy:4214 #-#-# translate crowdin weekend_date_lounge_890450ef:
msgid "[_R_] Should we go?"
