msgid "[_RT_] {i}I can't waste time right now, the DeCapos will be here any minute.{/i}"
msgstr "[_RT_] {i}Non posso perdere tempo in questo momento, il DeCapos sarà qui alcun minuto.{/i}"

# game/Phone_interface.rpy:276 #-#-# translate crowdin night_warning_48ae09e0:
msgid "[_RT_] {i}It's night, I should go to sleep.{/i}"
msgstr "[_RT_] {i}È notte, dovrei andare a dormire.{/i}"

# game/Phone_interface.rpy:281 #-#-# translate crowdin school_warning_1afbbf5b:
msgid "[_RT_] {i}Right now I'm at School, I must first go home to advance time.{/i}"
msgstr "[_RT_] {i}In questo momento sono a scuola, devo prima andare a casa per anticipare il tempo.{/i}"

# game/Phone_interface.rpy:286 #-#-# translate crowdin campaign_warning_88e99dc5:
msgid "[_RT_] {i}I can't waist time. I need to go to school and see how Lauren is doing in the student body election poll.{/i}"
msgstr "[_RT_] {i}Non posso stare in vita. Devo andare a scuola e vedere come sta facendo Lauren nel sondaggio elettorale del corpo studente.{/i}"

# game/Phone_interface.rpy:291 #-#-# translate crowdin prison_warning_5ae78a21:
msgid "[_RT_] {i}I can't waist time. I need to get ready to visit Dad in prison.{/i}"
msgstr "[_RT_] {i}Non posso stare in vita. Ho bisogno di prepararmi a visitare il papà in carcere.{/i}"

