# game/UpdateScript0.05.rpy:962
translate italian sidney_morning_jog_181c4c86:

    # S "Yeah right, I'll even give you a 1 minute head start."
    S "Sì bene, ti darò anche un inizio di 1 minuto."

# game/UpdateScript0.05.rpy:963
translate italian sidney_morning_jog_acd9e707:

    # S "On your mark... get set... go!"
    S "Sul tuo marchio... impostati... vai!"

# game/UpdateScript0.05.rpy:969
translate italian sidney_morning_jog_a7fb9998:

    # RT "{i}Oh, shit!.... I pushed it too hard, too fast.{/i}"
    RT "{i}Oh, merda!.... L'ho spinto troppo duramente, troppo veloce.{/i}"

# game/UpdateScript0.05.rpy:970
translate italian sidney_morning_jog_73608fa1:

    # RT "{i}I don't know if I can even make it to the park.{/i}"
    RT "{i}Non so se posso anche farlo al parco.{/i}"

# game/UpdateScript0.05.rpy:971
