    RT "{i}Oh wow! Ora non mi sento più così male!"

# game/UpdateScript0.05.rpy:1414
translate italian sidney_search_0007c522:

    # RT "{i}I never would have seen her in that outfit otherwise.{/i}"
    RT "{i}Non l'avrei mai vista con quel vestito altrimenti.{/i}"

# game/UpdateScript0.05.rpy:1417
translate italian sidney_search_8f0437de:

    # RT "{i}I sure wish I had access to the club's wardrobe.{/i}"
    RT "{i}Mi piacerebbe avere accesso al guardaroba del club.{/i}"

# game/UpdateScript0.05.rpy:1418
translate italian sidney_search_d015299a:

    # RT "{i}Mom pulls off the edgy look really well!{/i}"
    RT "{i}Mamma tira fuori il look tagliente davvero bene!"

# game/UpdateScript0.05.rpy:1421
translate italian sidney_search_28aa104e:

    # RT "{i}Oh God, but she looks even better when she's not wearing it at all.{/i}"
    RT "{i}Oh Dio, ma sta ancora meglio quando non lo indossa affatto.{/i}"

# game/UpdateScript0.05.rpy:1422
translate italian sidney_search_9dba7459:

    # RT "{i}How are her tits so perky? Did she even breastfeed us at all? Mmmm.... I wish I could remember that.{/i}"
    RT "{i}Come fanno le sue tette ad essere così vivaci? Ci ha almeno allattato? Mmmm.... Vorrei potermelo ricordare.{/i}"

# game/UpdateScript0.05.rpy:1423
