msgstr "[_M_] Je vous verrai bientôt."

# game/UpdateScript0.08.rpy:10958 #-#-# translate crowdin prison_visit_a2ddc80f:
msgid "[_RT_] {i}Ugghhh... No remote. Looks like I'm stuck watching two teams I know nothing about play soccer.{/i}"
msgstr "[_RT_] {i}Ugghhh... Pas de télécommande. On dirait que je suis coincé à regarder deux équipes dont je ne sais rien jouer au football.{/i}"

# game/UpdateScript0.08.rpy:10959 #-#-# translate crowdin prison_visit_2c7f6957:
msgid "[_RT_] {i}Could this day get any worse?{/i}"
msgstr "[_RT_] {i}Est-ce que cette journée pourrait être pire ?{/i}"

# game/UpdateScript0.08.rpy:10962 #-#-# translate crowdin prison_visit_3e860fc4:
