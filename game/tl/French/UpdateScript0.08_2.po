# game/UpdateScript0.08_2.rpy:4857 #-#-# translate crowdin seven_of_diamonds_6bf1c5f2:
msgid "{b}{i}\"View this card in the gallery and the casino\"{/i}{/b}"
msgstr "{b}{i}'Voir cette carte dans la galerie et le casino'{/i}{/b}"

# game/UpdateScript0.08_2.rpy:4875 #-#-# translate crowdin prison_visit_1dee2353:
msgid "{i}\"A little while later, on the way to the prison.\"{/i}"
