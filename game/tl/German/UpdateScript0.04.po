# game/UpdateScript0.04.rpy:2842 #-#-# translate crowdin assign_chores_d5a65542:
msgid "[_C_] Oh, fuck off. I'm not calling you sir!"
msgstr "[_C_] Oh, ficke ab! Ich rufe dich nicht an!"

# game/UpdateScript0.04.rpy:2843 #-#-# translate crowdin assign_chores_65ddfe61:
msgid "[_R_] Aaahh.... it was worth a try."
msgstr "[_R_] Aaahh.... es war einen Versuch wert."

# game/UpdateScript0.04.rpy:2844 #-#-# translate crowdin assign_chores_53df8203:
msgid "[_C_] Oh, and I'm going to finish my glass of wine before I start..."
msgstr "[_C_] Oh, und ich werde mein Glas Wein beenden, bevor ich starte..."

