# game/UpdateScript0.09.rpy:5519 #-#-# translate crowdin cami_events_intro_b10d177b:
msgid "[_C_] Let go of me!"
msgstr "Отпусти меня!"

# game/UpdateScript0.09.rpy:5520 #-#-# translate crowdin cami_events_intro_b1b8799c:
msgid "[_R_] Cami, listen..."
msgstr "[_R_] Cami, listen..."

# game/UpdateScript0.09.rpy:5521 #-#-# translate crowdin cami_events_intro_b76f6487:
