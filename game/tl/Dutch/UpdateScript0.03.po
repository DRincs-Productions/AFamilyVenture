# game/UpdateScript0.03.rpy:2473 #-#-# translate crowdin clubpool_80a4d8ea:
msgid "[_R_] I'll go see what I can do."
msgstr "[_R_] Ik zal gaan kijken wat ik kan doen."

# game/UpdateScript0.03.rpy:2480 #-#-# translate crowdin clubpool_e2aa607b:
msgid "[_RT_] {i}Yep, reading a book, just like always.{/i}"
msgstr "[_RT_] {i}Yep, lees een boek, net als altijd.{/i}"

# game/UpdateScript0.03.rpy:2481 #-#-# translate crowdin clubpool_b639dc18:
