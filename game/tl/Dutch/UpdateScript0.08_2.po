# game/UpdateScript0.08_2.rpy:9797 #-#-# translate crowdin sushi_date_d5ea9f2e:
msgid "[_RT_] {i}What!?... Do we?{/i}"
msgstr "[_RT_] {i}Wat?... Doen?{/i}"

# game/UpdateScript0.08_2.rpy:9798 #-#-# translate crowdin sushi_date_f6c5a897:
msgid "[_M_] It's my worthless, no good husband that got us into this mess!"
