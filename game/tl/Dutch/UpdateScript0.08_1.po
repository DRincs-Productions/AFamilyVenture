# game/UpdateScript0.08_1.rpy:13639 #-#-# translate crowdin girl_time_garden_3bd4e1cd:
msgid "[_S_] Well, we're all adults now."
msgstr "[_S_] Nou, we zijn nu alle volwassenen."

# game/UpdateScript0.08_1.rpy:13640 #-#-# translate crowdin girl_time_garden_5b34b985:
