# game/UpdateScriptChristmasEvent.rpy:1635 #-#-# translate crowdin christmas_shoot_7fbb3df6:
msgid "[_RT_] {i}Oh right!.... Whoops!{/i}"
msgstr "[ _RT_] {i}Oh right! .... Whoops!"

# game/UpdateScriptChristmasEvent.rpy:1636 #-#-# translate crowdin christmas_shoot_4e18b37a:
