    R "Haha... Ja, ich bin es, Frau Stein."

# game/UpdateScript0.07.rpy:4897
translate german stones_classroom_b2855d56_4:

    # MS "Come give me a hug."
    MS "Komm und gib mir einen Umarmung."

# game/UpdateScript0.07.rpy:4900
translate german stones_classroom_079926ad_4:

    # RT "{i}Mrs. Stone's hugs are the best!{/i}"
    RT "{i}Frau Steinflüsse sind die besten!{/i}"

# game/UpdateScript0.07.rpy:4907
translate german stones_classroom_3f62f859:

    # MS "Oh... Hello..."
    MS "Oh... Hello..."

# game/UpdateScript0.07.rpy:4908
translate german stones_classroom_4a378a4c:

    # R "Hi, Mrs. Stone."
    R "Hallo, Frau Stein."

# game/UpdateScript0.07.rpy:4909
translate german stones_classroom_7d20429c:

    # R "Remember me?"
    R "Denken Sie mich vergessen?"

# game/UpdateScript0.07.rpy:4910
translate german stones_classroom_d7d8db85:

    # MS "Oh... From my class last year."
    MS "Oh... Aus meiner Klasse letztes Jahr."

# game/UpdateScript0.07.rpy:4911
translate german stones_classroom_50cfee0d:

    # MS "Ronald, wasn't it?"
    MS "Ronald, nicht wahr?"

# game/UpdateScript0.07.rpy:4912
translate german stones_classroom_4856b932:

    # R "[ryan], actually."
    R "[ryan], eigentlich."

# game/UpdateScript0.07.rpy:4913
translate german stones_classroom_1018e65a:

    # MS "Oh yes, [ryan]."
    MS "Oh ja, [ryan]."

# game/UpdateScript0.07.rpy:4914
translate german stones_classroom_392b5acf:

