msgstr "[_RT_] {i}Uh, oh... Der Blick auf ihr Gesicht... Sie hat es nicht gefallen, dass ich das sage.{/i}"

# game/UpdateScript0.08_1.rpy:12174 #-#-# translate crowdin fixing_the_yard_d71fd891_1:
msgid "[_RT_] {i}Stupid, stupid, stupid!{/i}"
msgstr "[_RT_] {i}dumm, dumm, dumm!{/i}"

