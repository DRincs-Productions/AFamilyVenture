# game/UpdateScript0.08_2.rpy:5189 #-#-# translate crowdin prison_visit_149cec79:
msgid "[_R_] You too."
msgstr "[_R_] Sie auch."

# game/UpdateScript0.08_2.rpy:5190 #-#-# translate crowdin prison_visit_3bc357c1:
msgid "[_R_] I mean with your visit... Not with the conjugal... I mean..."
msgstr "[_R_] Ich meine mit deinem Besuch... Nicht mit Konjugal... Ich meine..."

# game/UpdateScript0.08_2.rpy:5191 #-#-# translate crowdin prison_visit_0ab18a79:
msgid "[_M_] Don't finish that."
msgstr "[_M_] Beende das nicht."

# game/UpdateScript0.08_2.rpy:5192 #-#-# translate crowdin prison_visit_6994f3c2:
