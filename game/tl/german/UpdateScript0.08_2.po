# game/UpdateScript0.08_2.rpy:9863 #-#-# translate crowdin sushi_date_e2b821c1_1:
msgid "[_J_] Oh, that's right."
msgstr "[_J_] Oh, das ist richtig."

# game/UpdateScript0.08_2.rpy:9866 #-#-# translate crowdin sushi_date_c87d4f0e_1:
