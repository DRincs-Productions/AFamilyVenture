# game/UpdateScript0.08_2.rpy:5202 #-#-# translate crowdin prison_visit_3e860fc4:
msgid "[_RT_] {i}Ohhh... It actually just got a whole lot better.{/i}"
msgstr "[_RT_] {i}Ohhhh... Es hat gerade eine ganze Menge besser.{/i}"

# game/UpdateScript0.08_2.rpy:5205 #-#-# translate crowdin prison_visit_dd5f19c3:
msgid "[_RT_] {i}That dress is so short.{/i}"
