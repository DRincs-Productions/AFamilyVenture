# game/UpdateScript0.08_2.rpy:531
translate german mom_bumps_sidney_continued_c4f3242d:

    # S "Thank you!"
    S "Ich danke Ihnen!"

# game/UpdateScript0.08_2.rpy:532
translate german mom_bumps_sidney_continued_d334e78d:

    # S "I'm really lucky you're my mom!"
    S "Ich bin wirklich glücklich, dass du mein Mädchen bist!"

# game/UpdateScript0.08_2.rpy:533
translate german mom_bumps_sidney_continued_af3fb427:

    # M "And I'm really lucky to have you for a daughter."
    M "Und ich bin wirklich glücklich, dich für eine Tochter zu haben."

# game/UpdateScript0.08_2.rpy:539
translate german mom_bumps_sidney_continued_62d98b0e:

    # RT "{i}Well, now that I'm feeling all warm and fuzzy, I'd better go to bed.{/i}"
    RT "{i}Nun, jetzt, da ich mich alles warm und unklar fühle, würde ich lieber ins Bett gehen.{/i}"

# game/UpdateScript0.08_2.rpy:542
translate german mom_bumps_sidney_continued_b7e81c57:

    # RT "{i}I'll miss that extra money Sidney's been bringing in each week, but on the other hand I couldn't be happier that she won't be working in the club anymore.{/i}"
    RT "{i}Ich vermisse das extra Geld Sidney jede Woche einbringt aber auf der anderen Seite konnte ich nicht glücklicher sein, dass sie nicht mehr im Club arbeitet. /i}"

# game/UpdateScript0.08_2.rpy:545
