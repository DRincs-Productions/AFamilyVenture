    # L "No"
    L "Non"

# game/UpdateScript0.05.rpy:4174
translate french breakfast_events_53cf3970:

    # S "No"
    S "Non"

# game/UpdateScript0.05.rpy:4177
translate french breakfast_events_6ce8ebfa:

    # R "Not really...."
    R "Pas vraiment...."

# game/UpdateScript0.05.rpy:4178
translate french breakfast_events_808d0609:

    # M "What's the matter, Sidney?"
    M "Qu'y a-t-il, Sidney ?"

# game/UpdateScript0.05.rpy:4179
translate french breakfast_events_62c1bd10:

    # M "You haven't even touched your breakfast."
    M "Tu n'as même pas touché à ton petit déjeuner."

# game/UpdateScript0.05.rpy:4180
translate french breakfast_events_0626bb64:

