    # RT "{i}I don't think she's even noticed I'm here... She really seems to be into that painting.{/i}"
    RT "{i}Je ne pense pas qu'elle ait même remarqué que je suis là... Elle semble vraiment être dans sa peinture.{/i}"

# game/UpdateScript0.07.rpy:4888
translate french stones_classroom_e60e57ff_4:

    # R "Uhhh... Mrs. Stone?"
    R "Uhhh... Mme Stone ?"

# game/UpdateScript0.07.rpy:4892
