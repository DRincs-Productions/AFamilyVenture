# game/UpdateScript0.07.rpy:4894
translate french stones_classroom_ea681a99:

    # R "Haha... Yeah, it's me, Mrs. Stone."
    R "Haha... Oui, c'est moi, Mme Stone."

# game/UpdateScript0.07.rpy:4897
translate french stones_classroom_b2855d56_4:

    # MS "Come give me a hug."
    MS "Viens me faire un câlin."

# game/UpdateScript0.07.rpy:4900
translate french stones_classroom_079926ad_4:

    # RT "{i}Mrs. Stone's hugs are the best!{/i}"
    RT "{i}Les câlins de Mme Stone sont les meilleurs!{/i}"

# game/UpdateScript0.07.rpy:4907
translate french stones_classroom_3f62f859:

    # MS "Oh... Hello..."
    MS "Oh... Bonjour..."

# game/UpdateScript0.07.rpy:4908
translate french stones_classroom_4a378a4c:

