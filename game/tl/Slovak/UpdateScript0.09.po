# game/UpdateScript0.09.rpy:5437 #-#-# translate crowdin cami_events_intro_d413d281:
msgid "[_C_] She treated me like I was her servant!"
msgstr "[_ C_] Ona ma liečila, ako by som bol jej sluha!"

# game/UpdateScript0.09.rpy:5438 #-#-# translate crowdin cami_events_intro_c8d60631:
msgid "[_C_] As for you, you little shit."
msgstr "[_C_] Ako pre teba, ty malý sráč."

# game/UpdateScript0.09.rpy:5439 #-#-# translate crowdin cami_events_intro_7ae66dfe:
