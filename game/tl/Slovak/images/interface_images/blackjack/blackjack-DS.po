# TODO: Translation updated at 2023-06-26 18:52 #|#|# # game/images/interface_images/blackjack/blackjack-DS.rpy:503 #-#-# translate crowdin startGameBJ_ddfcd22e:
msgid "[_BJ_dealer_] I'll start dealing the cards!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Začnem rozdávať karty!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:573 #-#-# translate crowdin handCheckLabel_8e5fda21:
msgid "[_BJ_dealer_] Dealer Bust, you won $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Krupiér vypadol, vyhral si $[betSum]! {w=2.0 }{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:585 #-#-# translate crowdin handCheckLabel_1a74bd53:
msgid "[_BJ_dealer_] You bust! You lost $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Vypadol si! Stratil si $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:597 #-#-# translate crowdin handCheckLabel_d1330db7:
msgid "[_BJ_dealer_] Blackjack! The dealer also has blackjack, but you have insurance! You win $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Blackjack! Krupiér má tiež blackjack, ale ty máš poistku! Vyhrávaš $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:600 #-#-# translate crowdin handCheckLabel_f74531ea:
msgid "[_BJ_dealer_] Blackjack! The dealer also has blackjack, so it's a push! You get your $[betSum] back!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Blackjack! Dealer má tiež blackjack, takže je to remíza! Dostaneš späť svoje $[betSum] !{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:612 #-#-# translate crowdin handCheckLabel_3b883a25:
msgid "[_BJ_dealer_] Blackjack! You won the game and $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Blackjack! Vyhral si hru a $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:623 #-#-# translate crowdin handCheckLabel_4b09cedb:
msgid "[_BJ_dealer_] Dealer has blackjack, but you have insurance! You make your bets back, $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Dealer má blackjack, ale ty máš poistku! Svoju stávku dostaneš späť, $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:626 #-#-# translate crowdin handCheckLabel_1d95a20c:
msgid "[_BJ_dealer_] Dealer has blackjack! You lose the game and $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Dealer má blackjack! Prehrávaš hru a $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:637 #-#-# translate crowdin handCheckLabel_b384deae:
msgid "[_BJ_dealer_] You won the game and $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Blackjack! Vyhral si hru a $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:649 #-#-# translate crowdin handCheckLabel_e4d35a85:
msgid "[_BJ_dealer_] You lost the game and $[betSum]!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Prehral si hru a $[betSum]!{w=2.0}{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:661 #-#-# translate crowdin handCheckLabel_901a9a2d:
msgid "[_BJ_dealer_] The dealer also has [computerScore], so it's a push! You get your $[betSum] back minus $[betIns] for insurance.{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Krupiér má tiež [computerScore], takže je to push! Dostaneš svoje $[betSum] späť mínus $[betIns] za poistenie. {w=2.0 }{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:663 #-#-# translate crowdin handCheckLabel_3615550f:
msgid "[_BJ_dealer_] The dealer also has [computerScore], so it's a push! You get your $[betSum] back!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Krupiér má tiež [computerScore], takže je to push! Dostaneš svoje $[betSum] späť! {w=2.0 }{nw}"

# game/images/interface_images/blackjack/blackjack-DS.rpy:681 #-#-# translate crowdin noMoney_9706b720:
msgid "[_BJ_dealer_] Sorry! You don't have enough money!{w=2.0}{nw}"
msgstr "[_BJ_dealer_] Prepáčte! Nemáte dosť peňazí!{w=2.0}{nw}"

# §translate crowdin strings: #|#|# # game/images/interface_images/blackjack/blackjack-DS.rpy:257
msgid "Dealer"
msgstr "Krupiér"

# game/images/interface_images/blackjack/blackjack-DS.rpy:367
msgid "Place your bets!"
msgstr "Uzatvárajte stávky!"

# game/images/interface_images/blackjack/blackjack-DS.rpy:374
msgid "Do you want to bet $100 and start the game?"
msgstr "Chcete staviť 100 dolárov a začať hru?"

# game/images/interface_images/blackjack/blackjack-DS.rpy:381
msgid "Do you want to bet $500 and start the game?"
msgstr "Chcete staviť 500 dolárov a začať hru?"

# game/images/interface_images/blackjack/blackjack-DS.rpy:388
msgid "Do you want to bet $1000 and start the game?"
msgstr "Chcete staviť 1000 dolárov a začať hru?"

# game/images/interface_images/blackjack/blackjack-DS.rpy:396
msgid "Do you want to leave the BlackJack game?"
msgstr "Chcete opustiť hru BlackJack?"

# game/images/interface_images/blackjack/blackjack-DS.rpy:404
msgid "Games won: [persistent.playerWin] / lost: [persistent.computerWin]"
msgstr "Vyhrané hry: [persistent.playerWin] / prehraté: [persistent.computerWin]"

# game/images/interface_images/blackjack/blackjack-DS.rpy:470
msgid "Player cards: [playerScore]"
msgstr "Karty hráča: [playerScore]"

# game/images/interface_images/blackjack/blackjack-DS.rpy:673
msgid "Do you want to play again?"
msgstr "Chcete znovu hrať?"

