# game/UpdateScriptChristmasEvent.rpy:739 #-#-# translate crowdin xmas_future_e8a946e5:
msgid "[_RT_] {i}Thank you! That is a pretty sweet gift.{/i}"
msgstr "[_RT_] {i}Ďakujeme! To je pekný darček. {/i}"

# game/UpdateScriptChristmasEvent.rpy:740 #-#-# translate crowdin xmas_future_b696d066:
